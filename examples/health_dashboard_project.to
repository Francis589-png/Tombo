```tombo
use bio_sensors
use env_sensors
use io
use json
use time
use core

# Health monitor module - collects sensor data

fn initialize_monitors()
    """Initialize all biometric sensors"""
    let monitors = {
        "heart_rate": initialize_heart_rate_monitor(sensor_type: "wearable"),
        "sensors": {}
    }
    return monitors

fn collect_health_metrics(monitors: Dict) -> Dict
    """Collect current health metrics from all sensors"""
    let timestamp = time_now()
    
    # Collect biometric data
    let metrics = {
        "timestamp": timestamp,
        "heart_rate": read_heart_rate(monitors["heart_rate"]),
        "blood_oxygen": read_blood_oxygen(monitors["heart_rate"]),
        "heart_rate_variability": read_heart_rate_variability(monitors["heart_rate"]),
        "blood_pressure": read_blood_pressure(monitors["heart_rate"]),
        "respiratory_rate": read_respiratory_rate(monitors["heart_rate"]),
        "stress_level": read_stress_level(monitors["heart_rate"]),
        "energy_level": read_energy_level(monitors["heart_rate"]),
        "mood": read_mood(monitors["heart_rate"])
    }
    
    return metrics

fn collect_environmental_data() -> Dict
    """Collect environmental context data"""
    let env_data = {
        "temperature": read_temperature(),
        "humidity": read_humidity(),
        "air_quality": read_air_quality(),
        "uv_index": read_uv_index(),
        "pressure": read_pressure()
    }
    
    return env_data

fn collect_activity_data(monitors: Dict) -> Dict
    """Collect daily activity metrics"""
    let activity = {
        "steps": read_step_count(monitors["heart_rate"]),
        "distance": read_distance(monitors["heart_rate"]),
        "calories_burned": read_calories_burned(monitors["heart_rate"]),
        "active_minutes": read_active_minutes(monitors["heart_rate"]),
        "sleep_data": read_sleep_data(monitors["heart_rate"]),
        "sleep_stages": read_sleep_stages(monitors["heart_rate"])
    }
    
    return activity

fn check_health_alerts(metrics: Dict, thresholds: Dict) -> List
    """Check if any metrics exceed safe thresholds"""
    let alerts = []
    
    if metrics["heart_rate"] > thresholds["max_heart_rate"]
        alerts = append(alerts, {
            "level": "warning",
            "message": "Heart rate elevated: " + str(metrics["heart_rate"]) + " bpm",
            "metric": "heart_rate",
            "value": metrics["heart_rate"]
        })
    
    if metrics["blood_oxygen"] < thresholds["min_blood_oxygen"]
        alerts = append(alerts, {
            "level": "danger",
            "message": "Low blood oxygen: " + str(metrics["blood_oxygen"]) + "%",
            "metric": "blood_oxygen",
            "value": metrics["blood_oxygen"]
        })
    
    if metrics["stress_level"] > thresholds["max_stress_level"]
        alerts = append(alerts, {
            "level": "warning",
            "message": "High stress level detected",
            "metric": "stress_level",
            "value": metrics["stress_level"]
        })
    
    return alerts

fn format_metrics_display(metrics: Dict) -> String
    """Format metrics for console display"""
    let output = ""
    output = output + "â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—\n"
    output = output + "â•‘      HEALTH METRICS (Live)            â•‘\n"
    output = output + "â• â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•£\n"
    output = output + "â•‘ Heart Rate:           " + str(metrics["heart_rate"]) + " bpm         â•‘\n"
    output = output + "â•‘ Blood Oxygen:         " + str(metrics["blood_oxygen"]) + "%           â•‘\n"
    output = output + "â•‘ Blood Pressure:       " + str(metrics["blood_pressure"]["systolic"]) + "/" + str(metrics["blood_pressure"]["diastolic"]) + "          â•‘\n"
    output = output + "â•‘ Respiratory Rate:     " + str(metrics["respiratory_rate"]) + " breaths/min    â•‘\n"
    output = output + "â•‘ Stress Level:         " + str(metrics["stress_level"]) + "/100           â•‘\n"
    output = output + "â•‘ Energy Level:         " + str(metrics["energy_level"]) + "/100           â•‘\n"
    output = output + "â•‘ Mood:                 " + str(metrics["mood"]) + "/10             â•‘\n"
    output = output + "â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n"
    
    return output

fn log_metrics_to_file(metrics: Dict, env_data: Dict, filename: String)
    """Log metrics to JSON file"""
    let log_entry = {
        "timestamp": metrics["timestamp"],
        "health": metrics,
        "environment": env_data
    }
    
    # Read existing log if it exists
    let existing_log = []
    if file_exists(path: filename)
        let content = read_file(path: filename)
        try
            existing_log = parse_json(content)
        catch error
            existing_log = []
    
    # Append new entry
    existing_log = append(existing_log, log_entry)
    
    # Write back to file
    write_file(path: filename, content: to_json(existing_log))
