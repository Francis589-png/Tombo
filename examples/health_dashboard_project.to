## File 3: data_analyzer.to

```tombo
use ml
use scientific
use io
use json
use collections

# Data analysis module - analyzes health trends

fn calculate_daily_average(metrics_list: List) -> Dict
    """Calculate daily averages from multiple readings"""
    if len(metrics_list) == 0
        return {}
    
    let hr_values = [m["heart_rate"] for m in metrics_list]
    let spo2_values = [m["blood_oxygen"] for m in metrics_list]
    let stress_values = [m["stress_level"] for m in metrics_list]
    
    let averages = {
        "avg_heart_rate": sum(hr_values) / len(hr_values),
        "min_heart_rate": min_value(hr_values),
        "max_heart_rate": max_value(hr_values),
        "avg_blood_oxygen": sum(spo2_values) / len(spo2_values),
        "avg_stress": sum(stress_values) / len(stress_values),
        "sample_count": len(metrics_list)
    }
    
    return averages

fn min_value(values: List) -> Int
    """Find minimum value in list"""
    let min_val = values[0]
    for val in values
        if val < min_val
            min_val = val
    return min_val

fn max_value(values: List) -> Int
    """Find maximum value in list"""
    let max_val = values[0]
    for val in values
        if val > max_val
            max_val = val
    return max_val

fn detect_health_trends(daily_averages: List) -> Dict
    """Detect trends in health data"""
    if len(daily_averages) < 3
        return {"status": "insufficient_data"}
    
    # Calculate heart rate trend
    let recent_avg = daily_averages[-1]["avg_heart_rate"]
    let previous_avg = daily_averages[-2]["avg_heart_rate"]
    let trend_days_back = daily_averages[-3]["avg_heart_rate"]
    
    let hr_trend = if recent_avg > previous_avg then "increasing" else "stable"
    let hr_change = recent_avg - previous_avg
    
    # Calculate stress trend
    let stress_recent = daily_averages[-1]["avg_stress"]
    let stress_prev = daily_averages[-2]["avg_stress"]
    let stress_trend = if stress_recent > stress_prev then "increasing" else "decreasing"
    
    let trends = {
        "heart_rate_trend": hr_trend,
        "heart_rate_change": hr_change,
        "stress_trend": stress_trend,
        "3_day_average_hr": (trend_days_back + previous_avg + recent_avg) / 3
    }
    
    return trends

fn calculate_health_score(metrics: Dict, activity: Dict) -> Dict
    """Calculate overall health score (0-100)"""
    let score = 100
    
    # Heart rate component (20%)
    if metrics["heart_rate"] > 100
        score = score - 5
    elif metrics["heart_rate"] > 120
        score = score - 10
    
    # Blood oxygen component (25%)
    if metrics["blood_oxygen"] < 95
        score = score - 15
    elif metrics["blood_oxygen"] < 98
        score = score - 5
    
    # Stress component (20%)
    if metrics["stress_level"] > 70
        score = score - 15
    elif metrics["stress_level"] > 50
        score = score - 5
    
    # Activity component (20%)
    if activity["steps"] < 5000
        score = score - 10
    elif activity["steps"] < 10000
        score = score - 5
    
    # Sleep component (15%)
    let sleep_hours = activity["sleep_data"]["duration"]
    if sleep_hours < 7
        score = score - 10
    elif sleep_hours > 9
        score = score - 5
    
    # Ensure score is between 0 and 100
    let final_score = if score < 0 then 0 else if score > 100 then 100 else score
    
    let assessment = if final_score >= 80 then "Excellent" 
                    elif final_score >= 60 then "Good" 
                    elif final_score >= 40 then "Fair" 
                    else "Poor"
    
    return {
        "score": final_score,
        "assessment": assessment,
        "components": {
            "heart_rate": metrics["heart_rate"],
            "blood_oxygen": metrics["blood_oxygen"],
            "stress": metrics["stress_level"],
            "activity": activity["steps"],
            "sleep": sleep_hours
        }
    }

fn generate_wellness_recommendations(metrics: Dict, activity: Dict, trends: Dict) -> List
    """Generate personalized wellness recommendations"""
    let recommendations = []
    
    # Heart rate recommendations
    if metrics["heart_rate"] > 100
        recommendations = append(recommendations, "Try relaxation techniques to reduce resting heart rate")
    
    # Activity recommendations
    if activity["steps"] < 10000
        recommendations = append(recommendations, "Aim for 10,000 steps daily - try a 15 minute walk")
    
    # Stress recommendations
    if metrics["stress_level"] > 60
        recommendations = append(recommendations, "Practice meditation for 10-15 minutes daily")
    
    # Sleep recommendations
    if activity["sleep_data"]["duration"] < 7
        recommendations = append(recommendations, "Maintain consistent sleep schedule (target 7-9 hours)")
    
    # Hydration
    recommendations = append(recommendations, "Stay hydrated - aim for 8 glasses of water daily")
    
    # Exercise
    if activity["active_minutes"] < 30
        recommendations = append(recommendations, "Get at least 30 minutes of moderate activity daily")
    
    # Oxygen saturation
    if metrics["blood_oxygen"] < 98
        recommendations = append(recommendations, "Ensure good air quality and consider deep breathing exercises")
    
    return recommendations

fn generate_daily_report(metrics: Dict, activity: Dict, health_score: Dict, 
                        recommendations: List) -> String
    """Generate formatted daily health report"""
    
    let report = ""
    report = report + "\nâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—\n"
    report = report + "â•‘           DAILY HEALTH REPORT                        â•‘\n"
    report = report + "â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n"
    
    # Health Score Section
    report = report + "ğŸ“Š HEALTH SCORE: " + str(health_score["score"]) + "/100 - " + health_score["assessment"] + "\n\n"
    
    # Vital Signs
    report = report + "ğŸ’“ VITAL SIGNS:\n"
    report = report + "  â€¢ Heart Rate: " + str(metrics["heart_rate"]) + " bpm\n"
    report = report + "  â€¢ Blood Oxygen: " + str(metrics["blood_oxygen"]) + "%\n"
    report = report + "  â€¢ Stress Level: " + str(metrics["stress_level"]) + "/100\n"
    report = report + "  â€¢ Energy Level: " + str(metrics["energy_level"]) + "/100\n\n"
    
    # Activity
    report = report + "ğŸƒ ACTIVITY:\n"
    report = report + "  â€¢ Steps: " + str(activity["steps"]) + " / 10,000\n"
    report = report + "  â€¢ Distance: " + str(activity["distance"]) + " km\n"
    report = report + "  â€¢ Calories: " + str(activity["calories_burned"]) + " kcal\n"
    report = report + "  â€¢ Active Time: " + str(activity["active_minutes"]) + " min\n\n"
    
    # Sleep
    report = report + "ğŸ˜´ SLEEP:\n"
    report = report + "  â€¢ Duration: " + str(activity["sleep_data"]["duration"]) + " hours\n"
    report = report + "  â€¢ Quality: " + str(activity["sleep_data"]["quality"]) + "/100\n\n"
    
